{
  "checks": {
    "service-b-health": {
      "type": "metric",
      "command": "/usr/lib64/nagios/plugins/check_http -N -H localhost -p 8080 -u /healthz",
      "subscribers": ["service-b"],
      "interval": 5,
      "refresh": 30,
      "handlers": [
        "slack",
        "influxdb",
        "opsgenie",
        "jira"
      ],
      "contacts": ["team-b"],
      "dependencies": [
        "network-device-0/network-device-health"
      ],
      "output_format": "nagios_perfdata"
    }
  }
}
