{
    "api_version": "core/v2",
    "type": "Handler",
    "metadata": {
        "namespace": "demo",
        "name": "slack"
    },
    "spec": {
        "type": "pipe",
        "runtime_assets": [
            "slack-handler"
        ],
        "command": "slack-handler --channel '#demo' --timeout 20 --username 'sensu' --webhook-url 'SECRET'",
        "timeout": 10,
        "filters": [
            "is_incident"
        ]
    }
}
